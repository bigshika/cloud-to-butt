{
	"manifest_version": 2,
    "name": "Gate to Fart",
    "version": "1.0",
    "description": "Replaces the text 'gate' with 'fart'.",
	"icons": {
           "32": "icon32.png",
           "48": "icon48.png"
       },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

